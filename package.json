{
  "name": "rup-api-satker",
  "version": "1.0.0",
  "description": "Backend API untuk data RUP (Rencana Umum Pengadaan) berdasarkan kd_satker menggunakan Node.js, Express.js, dan JSON API",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "build": "echo 'No build step required'",
    "test": "node --test",
    "lint": "echo 'No linting configured'",
    "clean": "rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean && npm install",
    "install-deps": "npm install --no-optional && npm install duckdb --optional"
  },
  "keywords": [
    "nodejs",
    "express",
    "json",
    "api",
    "rup",
    "cache"
  ],
  "author": "Rizko",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.5.0",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "os": ["linux", "darwin", "win32"],
  "cpu": ["x64", "arm64"]
}
